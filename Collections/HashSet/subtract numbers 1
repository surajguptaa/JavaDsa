import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
      Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int[] arr = new int[n];
        for(int i=0;i<n;i++){
            arr[i] = sc.nextInt();
        }
        HashSet<Integer>hs = new HashSet<>();
        for(int i=0;i<n;i++){
            if(arr[i]!=0)
            hs.add(arr[i]);
        }
        System.out.println(hs.size());
    }
}
