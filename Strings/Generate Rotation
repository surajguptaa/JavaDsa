import java.io.*;
import java.util.*;

public class Solution {
    public static void main(String[] args) {
       Scanner sc = new Scanner(System.in);
        String s = sc.nextLine();
        int n = s.length();
        for(int i=0;i<n;i++){
            String a = s.substring(0, n-i);
            String b = s.substring(n-i);
            //System.out.println(b+ "  "+a);
            System.out.println(b+a);
        }
    }
}
