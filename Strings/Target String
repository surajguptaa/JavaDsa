import java.io.*;
import java.util.*;

public class Solution {
    public static void main(String[] args) {
      Scanner sc = new Scanner(System.in);
        String str = sc.nextLine();
        String tar = sc.nextLine();
        str += str;
        if(str.contains(tar)){
            System.out.println("True");
        }
        else{
             System.out.println("False");
        }
        
    }
}
